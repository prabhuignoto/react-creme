{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "format": {},
    "lint": {},
    "build:lib": {},
    "build:doc": {},
    "build:story": {},
    "percy": {
      "outputs": ["storybook-static"]
    },
    "build": {
      "outputs": ["dist/**", "dts/**", "typings/**", "expo_dist"]
    },
    "test": {
      "outputs": ["coverage", ".jest-cache"]
    },
    "test:ci": {
      "outputs": ["coverage", ".jest-cache"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "ci": {
      "outputs": ["dist/**", "dts/**", "typings/**", "expo_dist", "coverage"]
    },
    "snyk": {},
    "fix-css": {},
    "fix-js": {}
  }
}
